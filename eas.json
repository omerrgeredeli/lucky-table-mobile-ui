{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "image": "latest"
      },
      "ios": {
        "simulator": false
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_USE_MOCK_API": "true",
        "EXPO_PUBLIC_ENV": "development"
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "image": "latest"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PUBLIC_USE_MOCK_API": "false",
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID_ANDROID": "your-prod-android-client-id",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID_IOS": "your-prod-ios-client-id",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID_WEB": "your-prod-web-client-id",
        "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET_WEB": "your-prod-web-client-secret"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}


